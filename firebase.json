{
  "functions": {
    "source": "./projects/cloud-functions",
    "predeploy": "yarn --prefix \"$RESOURCE_DIR\" run build"
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "emulators": {
    "pubsub": {
      "port": 5001
    },
    "functions": {
      "port": 5000
    },
    "firestore": {
      "port": 5002
    },
    "ui": {
      "enabled": true,
      "port": 4000
    }
  }
}
